<div class="wall">
  
  <div class="commits">
    <%- @entries.each do |entry| %>
      <%- if entry.class.name    == "Commit" %>
        <%= render :partial => "commit", :object => entry %>
      <%- elsif entry.class.name == "Comment" %>
        <%= render :partial => "comment", :object => entry %>
      <%- end %>
    <%- end %>
  </div>

  <div class="repositories">
    <%- @repositories.each do |repo| %>
      <%= render :partial => "repository", :object => repo %>
    <%- end %>
  </div>
  
  <div class="clear"></div>
  
</div>