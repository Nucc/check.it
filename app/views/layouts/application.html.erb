<!DOCTYPE html>
<html>
<head>
  <title>Reviewer</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults %>
  
  <%= javascript_include_tag "patches" %>
  
  <%= csrf_meta_tag %>
</head>
<body>

<% if notice %>
  <p class="notice"><%= notice %></p>
<% end %>

<% if alert %>
  <p class="alert"><%= alert %></p>
<% end %>


<div class="mainpage">
  <%= yield %>
</div>

</body>
</html>
